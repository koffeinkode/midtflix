  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "prisma": "^5.7.0",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0"
  }
}
