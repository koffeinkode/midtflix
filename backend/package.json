{
  "name": "midtflix-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend API for Midtflix",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
